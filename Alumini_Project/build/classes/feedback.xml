<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.pojo.Feedback" table="feedback">
<id name="fid" column="fid"></id>
<property name="teacherid" column="teacherid"></property>
<property name="stdid" column="stdid"></property>
<property name="rating" column="rating"></property>
<property name="remarks" column="remarks"></property>
</class>

</hibernate-mapping>