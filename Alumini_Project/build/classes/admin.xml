<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.pojo.Admin" table="administrator">
<id name="id" column="adminid"></id>
<property name="name" column="adminname"></property>
<property name="password" column="adminpassword"></property>
</class>
</hibernate-mapping>